<h2>List of TV series</h2>
<table class="table table-striped table-bordered">
  <tr>
    <th>Name</th>
    <th>Genre</th>
    <th>No. of seasons</th>
    <th>Release on</th>
    <th>Director</th>
    <th>Actor</th>
    <th>Shoot Location</th>
    <th>Country</th>
  </tr>
  <% @tv_series.each do |series| %>
    <tr>
      <td><%= series.name %></td>
      <td><%= series.genre %></td>
      <td><%= series.seasons %></td>
      <td><%= series.release_date %></td>
      <td><%= series.director %></td>
      <td><%= series.actor %></td>
      <td><%= series.shoot_location %></td>
      <td><%= series.country %></td>
    </tr>
  <% end %>
</table>

<hr>
<h2>Upload csv</h2>
<%= form_with url: '/tv_series', local: true do |f| %>
  <%= f.label :tv_serial_list %>
  <%= f.file_field :tv_serial_list %>
  <%= f.submit %>
<% end %>
